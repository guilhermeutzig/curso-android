<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="mainapplication.com.listas.DescriptionsLists"
    tools:showIn="@layout/activity_descriptions_lists"
    android:background="@color/colorPrimary">

    <ListView
        android:id="@+id/listaDescriptions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:listitem="@layout/celula_descricao"/>

    <Button
        android:id="@+id/goToHome"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@id/listaDescriptions"
        android:layout_centerHorizontal="true"
        android:text="Voltar"/>

</RelativeLayout>
